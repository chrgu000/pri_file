	//保存
	function save(){
	<%  foreach (ColumnSchema column in SourceTable.Columns) { if(column.IsPrimaryKeyMember||addIsContinue(column,dropDownListStr,0))continue;%>
			if($("#<%= column.Name %>").val()==""){
					$("#<%= column.Name %>").tips({
						side:3,
			            msg:'请输入<%= column.Description %>！',
			            bg:'#AE81FF',
			            time:2
			        });
					$("#<%= column.Name %>").focus();
					return false;
				}
									<% } %>
		$("#Form").submit();
		$("#zhongxin").hide();
		$("#zhongxin2").show();
	}
	
</script>
